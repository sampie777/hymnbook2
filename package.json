{
  "name": "hymnbook2",
  "version": "1.10.0-SNAPSHOT",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle": "cd android && ./gradlew bundleRelease",
    "build": "cd android && ./gradlew assembleRelease",
    "device-install": "adb -d install \"$(ls -t ./android/app/build/outputs/apk/release/*.apk | head -1)\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "2.2.2",
    "@react-native-clipboard/clipboard": "1.11.2",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "abcjs": "git+https://github.com/paulrosen/abcjs.git#6.2.2",
    "htmlparser2": "9.0.0",
    "react": "18.2.0",
    "react-native": "0.70.13",
    "react-native-config": "1.4.11",
    "react-native-device-info": "10.9.0",
    "react-native-gesture-handler": "2.10.0",
    "react-native-get-random-values": "1.8.0",
    "react-native-htmlview": "0.16.0",
    "react-native-keep-awake": "4.0.0",
    "react-native-quick-md5": "3.0.5",
    "react-native-reanimated": "2.17.0",
    "react-native-rsa-native": "2.0.5",
    "react-native-safe-area-context": "4.5.2",
    "react-native-screens": "3.18.2",
    "react-native-svg": "13.9.0",
    "react-native-vector-icons": "9.2.0",
    "realm": "11.3.1",
    "rollbar-react-native": "0.9.3"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/runtime": "7.21.5",
    "@types/jest": "29.5.1",
    "@types/react": "18.2.6",
    "@types/react-native": "0.70.13",
    "@types/react-native-htmlview": "0.16.0",
    "@types/react-native-keep-awake": "2.0.3",
    "@types/react-native-vector-icons": "6.4.13",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "29.5.0",
    "deprecated-react-native-prop-types": "4.1.0",
    "eslint": "8.40.0",
    "jest": "29.5.0",
    "metro-react-native-babel-preset": "0.75.1",
    "patch-package": "7.0.0",
    "react-native-codegen": "0.71.5",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "setupFiles": [
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/testSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(rollbar-react-native)"
    ]
  }
}
